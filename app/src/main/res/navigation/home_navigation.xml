<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/home_navigation"
    app:startDestination="@+id/nav_home_fragment">

    <fragment
        android:id="@+id/nav_home_fragment"
        android:name="com.project.debo.ui.HomeFragment"
        android:label="Inicio"
        tools:layout="@layout/fragment_home" >
    <action
        android:id="@+id/action_nav_home_fragment_to_nav_fragment_member_detail"
        app:destination="@id/nav_item_detail_fragment" />
    </fragment>

    <fragment
        android:id="@+id/nav_item_detail_fragment"
